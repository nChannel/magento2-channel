          "stock_id": {
            "type": "number"
          },
          "qty": {
            "type": "number"
          },
          "is_in_stock": {
            "type": "boolean"
          },
          "is_qty_decimal": {
            "type": "boolean"
          },
          "show_default_notification_message": {
            "type": "boolean"
          },
          "use_config_min_qty": {
            "type": "boolean"
          },
          "min_qty": {
            "type": "number"
          },
          "use_config_min_sale_qty": {
            "type": "number"
          },
          "min_sale_qty": {
            "type": "number"
